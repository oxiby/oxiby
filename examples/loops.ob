fn main() {
    let n = 10

    while n > 0 {
        print_line(n)
        n = n - 1
    }

    loop {
        print_line(n)
        n = n + 1

        if n > 9 {
            break
        }
    }

    for item in ["A", "B", "C"] {
        print_line(item)
    }

    for (key, value) in ["A" = 1, "B" = 2, "C" = 3] {
        print_line("#{key}: #{value}")
    }
}
