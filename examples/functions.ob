pub fn null() {}

pub fn greet(name: String) -> String {
    if name == "Oxiby" {
        return "Hello to myself!"
    }

    "Hello, #{name}!"
}

fn identity(t: t) -> t {
    t
}

trait AssocReq {
    type out is Requirement
}

trait GenericTrait<t> {
}

trait Add<rhs> where rhs = Self {
    type output

    fn add(self, other: rhs) -> output
}

impl Add<Integer> for Integer {
    fn add(self, other: rhs) -> Self {
        self + other
    }
}

fn add(a: a, b: a) -> a where a is Add {
    a + b
}

fn main() {
    null()
    greet("Oxiby")
    identity(true)
    add(1, 2)
}
